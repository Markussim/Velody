{
	"name": "velody",
	"version": "2.0.0-beta",
	"description": "Velody is a discord music bot!",
	"main": "src/main.ts",
	"scripts": {
		"build": "tsc",
		"start": "node dist/main.js",
		"dev": "nodemon -r dotenv/config src/main.ts dotenv_config_path=.env",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write src/**/*.{ts,js}"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/linusromland/Velody.git"
	},
	"keywords": [
		"Discord.js",
		"Velody",
		"Discord",
		"Music",
		"Bot"
	],
	"author": "Linus Romland (linusromland)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/linusromland/Velody/issues"
	},
	"homepage": "https://github.com/linusromland/Velody#readme",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.5.0",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@discordjs/voice": "^0.14.0",
		"@sapphire/framework": "^3.1.4",
		"@sapphire/plugin-logger": "^3.0.1",
		"discord.js": "^13.12.0",
		"dotenv": "^16.0.3",
		"ytdl-core": "^4.11.2",
		"ytsr": "^3.8.0"
	}
}
